<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GenericServletExample</servlet-name>
        <servlet-class>com.shamim.GenericServletExample</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExample</servlet-name>
        <servlet-class>com.shamim.ServletExample</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpServletHeaders</servlet-name>
        <servlet-class>com.shamim.HttpServletHeaders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FrontController</servlet-name>
        <servlet-class>com.shamim.webapplication.FrontController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>com.shamim.servlet.Servlet1</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.shamim.servlet.Servlet2</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenericServletExample</servlet-name>
        <url-pattern>/GenericServletExample</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExample</servlet-name>
        <url-pattern>/ServletExample</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpServletHeaders</servlet-name>
        <url-pattern>/HttpServletHeaders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FrontController</servlet-name>
        <url-pattern>/FrontController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
